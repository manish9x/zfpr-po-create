<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
>
    <TableSelectDialog
        title="{i18n>Select} {i18n>ExistingPurchaseOrder}"
        search=".handleVHExistPOSearch"
        confirm=".onExistPOVHSelected"
        cancel=".handleValueHelpClose"
        items="{ path : '/ZCDS_R_FPR_VH_EXISTING_PO'}"
        searchPlaceholder=""
        growingThreshold="40"
        multiSelect="true"
    >
        <ColumnListItem>
            <cells>
                <Text text="{ExistingPurchaseOrder}" />              
            </cells>
        </ColumnListItem>
        <columns>
            <Column>
                <header>
                    <Text text="Existing Purchase Order" />
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>